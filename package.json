{
  "name": "@daliborgogic/font-loading",
  "version": "1.1.0",
  "description": "Nuxt.js module for Font Loading Module Level 3 used for dynamically loading font resources.",
  "main": "lib/module.js",
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint --ext .js lib",
    "prerelease": "npm run lint",
    "release": "standard-version && git push origin master:publish --follow-tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daliborgogic/font-loading.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "font",
    "loading"
  ],
  "author": "Dalibor Gogic <mail@daliborgogic.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daliborgogic/font-loading/issues"
  },
  "homepage": "https://github.com/daliborgogic/font-loading#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "standard-version": "^4.4.0"
  }
}
